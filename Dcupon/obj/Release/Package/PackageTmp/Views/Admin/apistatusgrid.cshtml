@model Dcupon.Models.ApiUploadStatus



@if (Model.statuslist.Count != 0)
{
    for (int i = 0; i < Model.statuslist.Count; i++)
    {
        <tr>
            <td>@Model.statuslist[i].Date.ToShortDateString()</td>
            <td>@Model.statuslist[i].api_Name</td>
            <td>@Model.statuslist[i].api_import_status1</td>
            <td>@Model.statuslist[i].no_of_row </td>

        </tr>
    }
}
else
{
    <tr>
        <td>No Preview</td>
        <td>No Preview</td>
        <td>No Preview</td>
        <td>No Preview </td>

    </tr>
}

